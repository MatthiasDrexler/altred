{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .eslintignore --check --plugin-search-dir=. . && eslint --ignore-path .eslintignore .",
    "format": "prettier --ignore-path .eslintignore --write --plugin-search-dir=. .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest test src --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "cypress:run": "npx cypress run",
    "cypress:open": "TS_NODE_PROJECT=0 npx cypress open"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@sveltejs/adapter-node": "^1.0.0-next.53",
    "@sveltejs/kit": "^1.0.0-next.186",
    "@testing-library/jest-dom": "^5.14.0",
    "@testing-library/svelte": "^3.0.0",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-jest": "^27.0.0",
    "carbon-components-svelte": "^0.46.0",
    "carbon-icons-svelte": "^10.38.0",
    "carbon-preprocess-svelte": "^0.6.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jest": "^27.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.34.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.0.1",
    "svelte-preprocess": "^4.9.4",
    "ts-jest": "^27.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.4.1"
  }
}
